package org.liaohailong.spring.entity;

public class UserEntity {
	private String name;
	private int age;

	public UserEntity() {
		System.out.println("无参构造函数被调用");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	@Override
	public String toString() {
		return "UserEntity [name=" + name + ", age=" + age + "]";
	}
}
